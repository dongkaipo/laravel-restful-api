APP_NAME=LaravelRESTfulApi
APP_ENV=local

#Create APP_KEY by `php artisan key:generate`
APP_KEY=<key_value>

APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=root

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Ding/Api config
API_NAME="Laravel RESTful Api"
API_STANDARDS_TREE=vnd
API_SUBTYPE=LaravelRESTfulApi
API_DOMAIN=api.laravel.test
API_STRICT=false
API_DEBUG=true


# HashId Config
# Create value by `php vendor/bin/optimus spark`
HASHID_CONNECTION=optimus
OPTIMUS_PRIME=<prime_value>
OPTIMUS_INVERSE=<inverse_value>
OPTIMUS_RANDOM=<random_value>